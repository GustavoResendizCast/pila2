{
    "parser": "@babel/eslint-parser",
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["prettier"],
    "rules": {
        "no-console":"off",
        "prettier/prettier": ["error"]
    },
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "settings": {
        "import/resolver" :{ "babel-module":{}}
    },
    
    "globals": {
        "__rootdir": "readonly",
        "M": "writable"
      }
}
